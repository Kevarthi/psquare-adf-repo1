{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "psquare-adff-dev"
		},
		"AzureBlobStorage_SOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_SOURCE'"
		},
		"AzureDataLakeStorage_SINK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_SINK'"
		},
		"LS_AzureDataLakeStorage_SOURCE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage_SOURCE'"
		},
		"LS_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase'"
		},
		"AzureDataLakeStorage_SINK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://psquareadlsgen2storageac.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://psquare-kva.vault.azure.net/"
		},
		"LS_AzureDataLakeStorage_SOURCE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://psquareadlsgen2storageac.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "psquare-adff-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_SOURCE_connectionString": {
						"value": "[parameters('AzureBlobStorage_SOURCE_connectionString')]"
					},
					"AzureDataLakeStorage_SINK_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_SINK_accountKey')]"
					},
					"LS_AzureDataLakeStorage_SOURCE_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_SOURCE_accountKey')]"
					},
					"LS_AzureSqlDatabase_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase_connectionString')]"
					},
					"AzureDataLakeStorage_SINK_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_SINK_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_AzureDataLakeStorage_SOURCE_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_SOURCE_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}